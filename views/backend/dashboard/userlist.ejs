<%- include ('../partial/header') %>    
         <%- include ('../partial/sidebar') %>
         <!-- End Main Sidebar -->
         <%- include ('../partial/navbar') %>
         <div class="main-content-container container-fluid px-4">
            <!-- Page Header -->
            <div class="page-header row no-gutters py-4">
              <div class="col-12 col-sm-4 text-center text-sm-left mb-0">
                <!-- <a href="/dashboard/addmembership">Add Membership</a> -->
              </div>
            </div>
            <!-- End Page Header -->
            <!-- Default Light Table -->
            <div class="row">
              <div class="col">
                <div class="card card-small mb-4">
                  <div class="card-header border-bottom">
                    <h6 class="m-0">User Details</h6>
                  </div>
                  <div class="card-body p-0 pb-3 text-center">
                    <table class="table mb-0">
                      <thead class="bg-light">
                        <tr>
                          <th scope="col" class="border-0">Avatar</th>
                          <th scope="col" class="border-0">Name</th>
                          <th scope="col" class="border-0">Email</th>
                          <th scope="col" class="border-0">Phone</th>
                          <th scope="col" class="border-0">Bio</th>
                     
                          <th scope="col" class="border-0">City</th>
                          <th scope="col" class="border-0">State</th>
                          <th scope="col" class="border-0">Membership</th>
                          <th scope="col" class="border-0">Role</th>
                          <th scope="col" class="border-0">Action</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for(var i=0; i< data.length ; i++) { %>
                          <tr>
                            <td><img style="max-width: 2.5rem;" class="user-avatar rounded-circle mr-2" src="/backend/images/avatars/<%- data[i].pimage %>" alt="User Avatar"></td>
                          <td><%=  data[i].name %></td>
                          <td><%=  data[i].email %></td>
                          <td><%=  data[i].phone %></td>
                          <td><%=  data[i].bio %></td>
                          <td><%=  data[i].city %></td>
                          <td><%=  data[i].state %></td>
                          <td><%=  data[i].membership %></td>
                          <td><%=  data[i].title %></td>
                          <td><a href="/dashboard/userlist/<%=  data[i].id %>" class="btn btn-primary"><i class="fa fa-edit mx-2"></i></a>
                            <a href="/dashboard/deleteuser/<%=  data[i].id %>" class="btn btn-danger"><i class="fa fa-trash mx-2"></i></a>
                          </td>
                                              </tr>
                          <% } %>
                       
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
            <!-- End Default Light Table -->
            <!-- Default Dark Table -->
           
            <!-- End Default Dark Table -->
          </div>

          <%- include ('../partial/footer') %>